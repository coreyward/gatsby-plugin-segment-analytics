  "version": "0.1.1",
  "license": "MIT",
  "keywords": [
    "segment analytics",
    "analytics.js",
    "gatsby",
    "gatsby-plugin"
  ],
  "repository": {
    "url": "https://github.com/coreyward/gatsby-plugin-segment-analytics.git",
    "type": "git"
  },
  "dependencies": {
    "@segment/snippet": "^4.0.1",
    "babel-preset-es2015": "^7.0.0-beta.3"
  },
  "devDependencies": {
    "babel-cli": "^7.0.0-beta.3",
    "babel-preset-react": "^7.0.0-beta.3",
    "prettier": "^1.16",
    "react": "^16.8.0"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  }
